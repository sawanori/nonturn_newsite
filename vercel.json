{
  "rewrites": [
    { "source": "/",     "has": [{ "type": "host", "value": "foodphoto-pro.com" }],     "destination": "/services/photo/foodphoto" },
    { "source": "/form", "has": [{ "type": "host", "value": "foodphoto-pro.com" }],     "destination": "/services/photo/foodphoto/form" },
    { "source": "/",     "has": [{ "type": "host", "value": "www.foodphoto-pro.com" }], "destination": "/services/photo/foodphoto" },
    { "source": "/form", "has": [{ "type": "host", "value": "www.foodphoto-pro.com" }], "destination": "/services/photo/foodphoto/form" }
  ],
  "redirects": [
    { "source": "/:path((?!form$).*)", "has": [{ "type": "host", "value": "foodphoto-pro.com" }],     "destination": "https://non-turn.com/:path*", "permanent": true },
    { "source": "/:path((?!form$).*)", "has": [{ "type": "host", "value": "www.foodphoto-pro.com" }], "destination": "https://non-turn.com/:path*", "permanent": true }
  ]
}